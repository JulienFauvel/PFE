<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

    <title>
        {% block title %}Admin Panel{% endblock %}
    </title>

    {% block stylesheets %}{% endblock %}

    <link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}"/>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
{% block navbar %}
    <nav class="navbar navbar-default">
        <div class="container-fluid">

            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                        data-target="#navbar-collapse" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="{{ path('admin') }}">Admin Panel</a>
            </div>


            <div class="collapse navbar-collapse" id="navbar-collapse">
                <ul class="nav navbar-nav">
                    <li><a href="/restaurants">Users</a></li>
                    <li><a href="/voyages">Voyages</a></li>
                    <li><a href="/bonplans">Bon plans</a></li>
                </ul>

            </div>
        </div>
    </nav>
{% endblock %}

{% block left_panel %}
    <div class="col-lg-2 col-sm-0"></div>
{% endblock %}

<div class="col-lg-8">
    {% block content %}{% endblock %}
</div>


{% block right_panel %}
    <div class="col-lg-2 col-sm-0"></div>
{% endblock %}


<script type="application/javascript" src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
<script type="application/javascript" src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>
{% block javascripts %}{% endblock %}
</body>
</html>
